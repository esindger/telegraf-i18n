{
  "name": "@esindger/telegraf-i18n",
  "version": "0.0.1",
  "description": "Telegraf i18n engine",
  "main": "dist/source",
  "types": "dist/source",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/esindger/telegraf-i18n.git"
  },
  "keywords": [
    "telegram bot",
    "telegraf",
    "bot framework",
    "i18n",
    "internationalization",
    "middleware"
  ],
  "author": "Vitaly Domnikov <oss@vitaly.codes>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esindger/telegraf-i18n/issues"
  },
  "homepage": "https://github.com/esindger/telegraf-i18n#readme",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/source",
    "!*.test.*"
  ],
  "scripts": {
    "build": "del-cli dist && tsc",
    "prepack": "npm run build",
    "start": "ts-node examples/example-bot.ts",
    "test": "tsc --sourceMap && nyc ava"
  },
  "dependencies": {
    "abstract-syntax-tree": "^2.15.3",
    "compile-template": "^0.3.1",
    "debug": "^4.0.1",
    "js-yaml": "^4.0.0",
    "tableize-object": "^0.1.0"
  },
  "peerDependencies": {
    "telegraf": "^4.0.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.8.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.0.5",
    "ava": "^3.0.0",
    "del-cli": "^3.0.0",
    "nyc": "^15.0.0",
    "telegraf": "^4.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "dist/source",
      "source"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
